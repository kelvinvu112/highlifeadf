{
	"name": "pipeline1",
	"properties": {
		"activities": [
			{
				"name": "SalesForce Accounts to REST API",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SalesforceSource",
						"additionalColumns": [
							{
								"name": "RecordSource",
								"value": {
									"value": "Salesforce",
									"type": "Expression"
								}
							}
						],
						"readBehavior": "query"
					},
					"sink": {
						"type": "RestSink",
						"httpRequestTimeout": "00:05:00",
						"requestInterval": 10,
						"requestMethod": "PATCH",
						"writeBatchSize": "500",
						"httpCompressionType": "none"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "Id",
									"type": "String"
								},
								"sink": {
									"path": "$['Code']"
								}
							},
							{
								"source": {
									"name": "Name",
									"type": "String"
								},
								"sink": {
									"path": "$['Name']"
								}
							},
							{
								"source": {
									"name": "BillingStreet",
									"type": "String"
								},
								"sink": {
									"path": "$['BillingStreet']"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "SalesforceObject1",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "ProfiseeRESTAPI",
						"type": "DatasetReference",
						"parameters": {
							"entityId": "Account"
						}
					}
				]
			}
		],
		"folder": {
			"name": "Salesforce"
		},
		"annotations": []
	}
}