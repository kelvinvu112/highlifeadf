{
	"name": "Lead Copy Data",
	"properties": {
		"activities": [
			{
				"name": "Lead Entity Copy Data",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SalesforceSource",
						"query": "SELECT Id,Name,Phone,LastModifiedDate,Acton_Lead_Score__c,Email,Company,Bounced_Email__c,Fax,HasOptedOutOfEmail,FirstName,LastName,City,Country,State,Street,PostalCode,OwnerId,Pardot_Score_Reset__c,Title from Lead LIMIT 1",
						"readBehavior": "query"
					},
					"sink": {
						"type": "RestSink",
						"httpRequestTimeout": "00:05:00",
						"requestInterval": 10,
						"requestMethod": "PATCH",
						"writeBatchSize": "500",
						"httpCompressionType": "none"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "Id",
									"type": "String"
								},
								"sink": {
									"path": "$['Code']"
								}
							},
							{
								"source": {
									"name": "LastModifiedDate",
									"type": "DateTime"
								},
								"sink": {
									"path": "$['SF Last Modified Date']"
								}
							},
							{
								"source": {
									"name": "Bounced_Email__c",
									"type": "Boolean"
								},
								"sink": {
									"path": "$['Bounced Email']"
								}
							},
							{
								"source": {
									"name": "Fax",
									"type": "String"
								},
								"sink": {
									"path": "$['BusinessFax']"
								}
							},
							{
								"source": {
									"name": "FirstName",
									"type": "String"
								},
								"sink": {
									"path": "$['First Name']"
								}
							},
							{
								"source": {
									"name": "LastName",
									"type": "String"
								},
								"sink": {
									"path": "$['Last Name']"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "SalesforceObject1",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "ProfiseeRESTAPI",
						"type": "DatasetReference",
						"parameters": {
							"entityId": "Lead"
						}
					}
				]
			}
		],
		"folder": {
			"name": "Salesforce"
		},
		"annotations": []
	}
}