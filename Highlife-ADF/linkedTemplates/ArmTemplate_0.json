{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Highlife-ADF"
		},
		"Salesforce1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Salesforce1'"
		},
		"Salesforce1_securityToken": {
			"type": "secureString",
			"metadata": "Secure string for 'securityToken' of 'Salesforce1'"
		},
		"ProfiseeRESTAPI_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://highlifedev.services.profisee.com/profisee/rest/v1"
		},
		"Salesforce1_properties_typeProperties_environmentUrl": {
			"type": "string",
			"defaultValue": "https://profisee.my.salesforce.com/"
		},
		"Salesforce1_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "lee.gregory@profisee.com"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/ProfiseeRESTAPI')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "RestService",
				"typeProperties": {
					"url": "[parameters('ProfiseeRESTAPI_properties_typeProperties_url')]",
					"enableServerCertificateValidation": true,
					"authenticationType": "Anonymous",
					"authHeaders": {}
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/Salesforce1')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "Salesforce",
				"typeProperties": {
					"environmentUrl": "[parameters('Salesforce1_properties_typeProperties_environmentUrl')]",
					"username": "[parameters('Salesforce1_properties_typeProperties_username')]",
					"password": {
						"type": "SecureString",
						"value": "[parameters('Salesforce1_password')]"
					},
					"securityToken": {
						"type": "SecureString",
						"value": "[parameters('Salesforce1_securityToken')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/ProfiseeRESTAPI')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "ProfiseeRESTAPI",
					"type": "LinkedServiceReference"
				},
				"parameters": {
					"entityId": {
						"type": "string",
						"defaultValue": "Account"
					}
				},
				"annotations": [],
				"type": "RestResource",
				"typeProperties": {
					"relativeUrl": ""
				},
				"schema": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/ProfiseeRESTAPI')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/SalesforceObject1')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "Salesforce1",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "SalesforceObject",
				"schema": [],
				"typeProperties": {
					"objectApiName": "Account"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/Salesforce1')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/pipeline1')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "SalesForce Accounts to REST API",
						"type": "Copy",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"source": {
								"type": "SalesforceSource",
								"additionalColumns": [
									{
										"name": "Record Source",
										"value": {
											"value": "Salesforce",
											"type": "Expression"
										}
									}
								],
								"readBehavior": "query"
							},
							"sink": {
								"type": "RestSink",
								"httpRequestTimeout": "00:01:40",
								"requestInterval": 10,
								"requestMethod": "PATCH",
								"writeBatchSize": 10000,
								"httpCompressionType": "none"
							},
							"enableStaging": false,
							"translator": {
								"type": "TabularTranslator",
								"mappings": [
									{
										"source": {
											"name": "Id",
											"type": "String"
										},
										"sink": {
											"path": "$['Code']"
										}
									},
									{
										"source": {
											"name": "Name",
											"type": "String"
										},
										"sink": {
											"path": "$['Name']"
										}
									},
									{
										"source": {
											"name": "Phone",
											"type": "String"
										},
										"sink": {
											"path": "$['Phone']"
										}
									},
									{
										"source": {
											"name": "Account_Territory__c",
											"type": "String"
										},
										"sink": {
											"path": "$['Account Territory']"
										}
									},
									{
										"source": {
											"name": "BillingCity",
											"type": "String"
										},
										"sink": {
											"path": "$['BillingCity']"
										}
									},
									{
										"source": {
											"name": "BillingCountry",
											"type": "String"
										},
										"sink": {
											"path": "$['BillingCountry']"
										}
									},
									{
										"source": {
											"name": "BillingState",
											"type": "String"
										},
										"sink": {
											"path": "$['BillingStateProvince']"
										}
									},
									{
										"source": {
											"name": "BillingStreet",
											"type": "String"
										},
										"sink": {
											"path": "$['BillingStreet']"
										}
									},
									{
										"source": {
											"name": "BillingPostalCode",
											"type": "String"
										},
										"sink": {
											"path": "$['BillingZipPostalCode']"
										}
									},
									{
										"source": {
											"name": "CAB_Member__c",
											"type": "Boolean"
										},
										"sink": {
											"path": "$['CAB Member']"
										}
									},
									{
										"source": {
											"name": "CAM__c",
											"type": "String"
										},
										"sink": {
											"path": "$['CAM']"
										}
									},
									{
										"source": {
											"name": "Customer_Status__c",
											"type": "String"
										},
										"sink": {
											"path": "$['Customer Status']"
										}
									},
									{
										"source": {
											"name": "License_Expiration_Date__c",
											"type": "DateTime"
										},
										"sink": {
											"path": "$['Expiration Date']"
										}
									},
									{
										"source": {
											"name": "License_Expiration_Date_AV__c",
											"type": "DateTime"
										},
										"sink": {
											"path": "$['Expiration Date - AV']"
										}
									},
									{
										"source": {
											"name": "GRM_Server__c",
											"type": "Boolean"
										},
										"sink": {
											"path": "$['GRM Server']"
										}
									},
									{
										"source": {
											"name": "Industry",
											"type": "String"
										},
										"sink": {
											"path": "$['Industry']"
										}
									},
									{
										"source": {
											"name": "Initial_Contract_Date__c",
											"type": "DateTime"
										},
										"sink": {
											"path": "$['First Purchase Date']"
										}
									},
									{
										"source": {
											"name": "Federation_Manager__c",
											"type": "Boolean"
										},
										"sink": {
											"path": "$['Integrator']"
										}
									},
									{
										"source": {
											"name": "MDS_Support_Annivers__c",
											"type": "DateTime"
										},
										"sink": {
											"path": "$['MDS Support Anniversary']"
										}
									},
									{
										"source": {
											"name": "MSFT_Partner__c",
											"type": "String"
										},
										"sink": {
											"path": "$['PartnerMicrosoft']"
										}
									},
									{
										"source": {
											"name": "Netsuite_ID__c",
											"type": "String"
										},
										"sink": {
											"path": "$['Customer Number']"
										}
									},
									{
										"source": {
											"name": "OwnerId",
											"type": "String"
										},
										"sink": {
											"path": "$['SF Account Owner']"
										}
									},
									{
										"source": {
											"name": "Partner_Status__c",
											"type": "String"
										},
										"sink": {
											"path": "$['Partner Status']"
										}
									},
									{
										"source": {
											"name": "Premium_DQ__c",
											"type": "Boolean"
										},
										"sink": {
											"path": "$['Premium DQ']"
										}
									},
									{
										"source": {
											"name": "Profisee_MDM_Support_Anniversary__c",
											"type": "DateTime"
										},
										"sink": {
											"path": "$['Maestro Support Anniversary']"
										}
									},
									{
										"source": {
											"name": "Profisee_MDM_Version__c",
											"type": "String"
										},
										"sink": {
											"path": "$['Maestro Version']"
										}
									},
									{
										"source": {
											"name": "SDK__c",
											"type": "Boolean"
										},
										"sink": {
											"path": "$['SDK']"
										}
									},
									{
										"source": {
											"name": "Professional_Services_Owner__c",
											"type": "String"
										},
										"sink": {
											"path": "$['Services Owner']"
										}
									},
									{
										"source": {
											"name": "TickerSymbol",
											"type": "String"
										},
										"sink": {
											"path": "$['TickerSymbol']"
										}
									},
									{
										"source": {
											"name": "Website",
											"type": "String"
										},
										"sink": {
											"path": "$['Website']"
										}
									}
								]
							}
						},
						"inputs": [
							{
								"referenceName": "SalesforceObject1",
								"type": "DatasetReference",
								"parameters": {}
							}
						],
						"outputs": [
							{
								"referenceName": "ProfiseeRESTAPI",
								"type": "DatasetReference",
								"parameters": {
									"entityId": "Account"
								}
							}
						]
					}
				],
				"folder": {
					"name": "Salesforce"
				},
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/SalesforceObject1')]",
				"[concat(variables('factoryId'), '/datasets/ProfiseeRESTAPI')]"
			]
		}
	]
}